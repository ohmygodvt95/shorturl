<body>
  <%= render "layouts/header" %>
  <div class="wrapper">
    <%= yield %>

  </div>
  <div class="container">
    <%= debug params if Rails.env.development? %>
  </div>
  <%= render "layouts/footer" %>
  <%= javascript_include_tag "application"%>
  <%= render "layouts/flash" %>

</body>
